{
  "name": "whatsapp-contact-bot",
  "version": "1.0.0",
  "description": "Telegram bot to create WhatsApp chat messages with people not in your contacts",
  "scripts": {
    "webhook-setup": "env-cmd -x curl --request POST --url https://api.telegram.org/bot\\$TELEGRAM_BOT_TOKEN/setWebhook --header 'content-type: application/json' --data '{\"url\": \"$NETLIFY_URL\"}'",
    "typecheck": "tsc --noEmit"
  },
  "author": "Sebastian Quek",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@netlify/functions": "^1.0.0",
    "telegraf": "^4.7.0"
  }
}
